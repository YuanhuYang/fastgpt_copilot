# 开发环境配置示例
# 复制为 development.env 并修改相应配置

# GitHub Copilot配置
GITHUB_TOKEN=your_github_token_here

# Copilot Proxy配置
PORT=8888
CORS_ORIGIN=*
LOG_LEVEL=debug
NODE_ENV=development

# 数据库配置
MONGO_USERNAME=dev
MONGO_PASSWORD=dev123
PG_USERNAME=dev
PG_PASSWORD=dev123
PG_DATABASE=fastgpt_dev

# FastGPT配置
FASTGPT_ROOT_PASSWORD=dev123
CHAT_API_KEY=your-fastgpt-copilot-api-key
TOKEN_KEY=dev_token_key
ROOT_KEY=dev_root_key
FILE_TOKEN_KEY=dev_file_token_key

# 其他配置
COMPOSE_PROJECT_NAME=fastgpt-copilot-dev
